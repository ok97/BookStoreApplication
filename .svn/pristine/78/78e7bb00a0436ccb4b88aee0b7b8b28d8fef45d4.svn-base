

CREATE TABLE [dbo].[Users](
	UserId int IDENTITY(1,1) NOT NULL,
	FirstName varchar(20),
	LastName varchar(20) ,
	Email varchar(30) ,
	Password varchar(30) ,
	Role varchar(30),
PRIMARY KEY (UserId) );


